[
  { "key": "cmd+[", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
  { "key": "cmd+\\", "command": "workbench.action.toggleSidebarVisibility" },
  { "key": "cmd+shift+\\", "command": "workbench.files.action.showActiveFileInExplorer"},
  { "key": "cmd+shift+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
  { "key": "shift+cmd+-", "command": "workbench.action.previousEditor" },
  { "key": "shift+cmd+=", "command": "workbench.action.nextEditor" }
]
