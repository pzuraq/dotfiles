{
    "gitlens.codeLens.enabled": false,
    "vscode_custom_css.imports": [
        "file:///Users/pzuraq/.vscode/extensions/pzuraq.vertical-tab-bar-1.0.0/styles.css"
    ],
    "javascript.validate.enable": false,
    "editor.formatOnSave": false,
    "search.useRipgrep": false,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.node_modules.ember-try": true,
        "**/.svn": true,
        "**/buck-out": true,
        "**/target": true,
        "**/tmp": true,
    },
    "search.exclude": {
        "**/.bower_components.ember-try": true,
        "**/.git": true,
        "**/.idea": true,
        "**/.node_modules.ember-try": true,
        "**/bower_components": true,
        "**/buck-out": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/out": true,
        "**/target": true,
        "**/tmp": true,
        "**/vendor": true,
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.node_modules.ember-try": true,
        "**/node_modules/**": true,
        "**/target/**": true,
        "**/tmp/**/*": true,
    },
    "openInGitHub.defaultBranch": "master",
    "openInGitHub.defaultRemote": "origin",
    "editor.renderWhitespace": true,
    "editor.renderIndentGuides": true,
    "editor.tabSize": 2,
    "editor.wordWrap": true,
    "explorer.autoReveal": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "jsDocCompletion.enabled": false,
    "guides.normal.color.light": "rgba(35, 65, 84, 1)",
    "guides.stack.color.light": "rgba(35, 65, 84, 1)",
    "jshint.enable": false,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Oceanic Reef Dark",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.welcome.enabled": false,
    "explorer.openEditors.visible": 0,
    "gitlens.blame.annotation.activeLine": "off",
    "editor.minimap.enabled": false,
    "emmet.showExpandedAbbreviation": "never",
    // "workbench.iconTheme": "material-icon-theme",
    // "window.titleBarStyle": "native",
    // "editor.parameterHints": true,
    // "gitlens.blame.line.enabled": false,
