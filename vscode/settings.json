{
    "gitlens.codeLens.enabled": false,
    "vscode_custom_css.imports": [
        "file:///Users/pzuraq/.vscode/extensions/pzuraq.vertical-tab-bar-0.1.0/styles/vertical-tab-bar.css"
    ],

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/tmp": true,
        "**/.idea": true,
        "**/buck-out": true,
        "**/.node_modules.ember-try": true
    },

    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/.node_modules.ember-try": true,
        "**/bower_components": true,
        "**/.bower_components.ember-try": true,
        "**/vendor": true,
        "**/dist": true,
        "**/tmp": true,
        "**/.idea": true,
        "**/buck-out": true,
        "**/out": true
    },

    "search.useRipgrep": false,

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/tmp": true,
        "/tmp/**/*": true,
        "**/.node_modules.ember-try": true
    },

    "openInGitHub.defaultBranch": "master",
    "openInGitHub.defaultRemote": "origin",

    "editor.renderWhitespace": true,
    "editor.renderIndentGuides": true,
    "editor.tabSize": 2,
    "editor.wordWrap": true,

    "explorer.autoReveal": false,

    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,

    "guides.normal.color.light": "rgba(35, 65, 84, 1)",
    "guides.stack.color.light": "rgba(35, 65, 84, 1)",

    "jshint.enable": false,

    "window.zoomLevel": 0,

    "workbench.colorTheme": "FlatUI Immersed",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.welcome.enabled": false,

    "explorer.openEditors.visible": 0,
    "gitlens.blame.annotation.activeLine": "off",

    "editor.minimap.enabled": false,

