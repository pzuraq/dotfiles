{
  "vscode_custom_css.imports": [
    "file:///Users/cgarrett/.dotfiles/vscode/styles.css",
    "file:///Users/cgarrett/.dotfiles/vscode/script.js",
    "file:///Users/pzuraq/.dotfiles/vscode/styles.css",
    "file:///Users/pzuraq/.dotfiles/vscode/script.js"
  ],
  "vscode_custom_css.policy": true,
  "javascript.validate.enable": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": true
  },
  "search.useRipgrep": false,
  "git.path": "/usr/local/bin/git",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.node_modules.ember-try": true,
    "**/.svn": true,
    "**/buck-out": true,
    "**/target": true
  },
  "search.exclude": {
    "**/.bower_components.ember-try": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.node_modules.ember-try": true,
    "**/package.json.ember-try": true,
    "**/bower_components": true,
    "**/buck-out": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/out": true,
    "**/target": true,
    "**/tmp": true,
    "**/vendor": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.node_modules.ember-try": true,
    "**/node_modules/**": true,
    "**/target/**": true,
    "**/tmp/**/*": true
  },
  "openInGitHub.defaultBranch": "master",
  "openInGitHub.defaultRemote": "origin",
  "editor.renderWhitespace": "none",
  "editor.renderIndentGuides": true,
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "jsDocCompletion.enabled": false,
  "guides.normal.color.light": "rgba(35, 65, 84, 1)",
  "guides.stack.color.light": "rgba(35, 65, 84, 1)",
  "jshint.enable": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.welcome.enabled": false,
  "explorer.openEditors.visible": 100,
  "gitlens.blame.annotation.activeLine": "off",
  "editor.minimap.enabled": false,
  "typescript.check.npmIsInstalled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "workbench.activityBar.visible": true,
  "gitlens.keymap": "alternate",
  "rust-client.channel": "nightly",
  "editor.rulers": [80],

  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "todohighlight.isCaseSensitive": false,
  "todohighlight.keywords": [
    "obviously",
    "basically",
    "simply",
    "of course",
    "clearly",
    "just",
    "everyone knows",
    "however",
    " so ",
    " so.",
    " so,",
    "easy"
  ],
  "gitlens.historyExplorer.enabled": true,

  "workbench.sideBar.location": "left",
  "github.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "materialTheme.autoApplyIcons": false,
  "git.ignoreMissingGitWarning": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "liveshare.featureSet": "insiders",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "importSorter.generalConfiguration.sortImportsInDirectory": false,
  "importSorter.sortConfiguration.customOrderingRules.defaultNumberOfEmptyLinesAfterGroup": 0,
  "importSorter.sortConfiguration.removeUnusedImports": true,
  "importSorter.sortConfiguration.removeUnusedDefaultImports": true,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
  "importSorter.importStringConfiguration.tabSize": 2,
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "revealjs.width": "100%",
  "revealjs.maxScale": 1,
  "revealjs.height": "100%",
  "revealjs.minScale": 1,
  "revealjs.transition": "none",
  "revealjs.enableMenu": false,
  "revealjs.enableTitleFooter": false,
  "revealjs.enableChalkBoard": false,
  "revealjs.enableChalkboard": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.autoUpdate": false,
  "revealjs.theme": "night",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.configuration.checkProjectSettingsExclusions": false,
  "terminal.integrated.scrollback": 10000,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
  ],
  "references.preferredLocation": "view",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "eslint.format.enable": true,
  "workbench.editor.wrapTabs": true,
  "workbench.colorTheme": "Night Owl",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "MonoLisa, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "fontshortcuts.step": 0.5,
  "editor.lineHeight": 0,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 12,
  "editor.wordWrap": "on",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "rust-analyzer.checkOnSave.command": "check --all-targets --tests",
  "rust-analyzer.checkOnSave.allTargets": false,
  "svelte.enable-ts-plugin": true,
  "editor.formatOnSave": true
}
